<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/muchacomida"
    tools:context=".MainActivity5">

    <TableLayout
        android:id="@+id/tl4"
        android:layout_width="346dp"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:id="@+id/tr113"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:minHeight="150dp">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="100dp"
                android:layout_height="134dp"
                app:srcCompat="@drawable/icono_fondo" />
        </TableRow>

        <TableRow
            android:id="@+id/tr_111"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:minHeight="70dp">

            <EditText
                android:id="@+id/ed_nombre"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/h1"
                android:inputType="textPersonName" />
        </TableRow>

        <TableRow
            android:id="@+id/tr112"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:minHeight="70dp">

            <EditText
                android:id="@+id/ed_password"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/h2"
                android:inputType="textPassword" />
        </TableRow>

        <TableRow
            android:id="@+id/tr114"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:minHeight="70dp">

            <EditText
                android:id="@+id/ed_password2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/h6"
                android:inputType="textPassword" />
        </TableRow>

        <TableRow
            android:id="@+id/tr_116"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:minHeight="70dp">

            <EditText
                android:id="@+id/ed_8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName" />
        </TableRow>

        <TableRow
            android:id="@+id/tr_9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:minHeight="70dp">

            <EditText
                android:id="@+id/ed_9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="phone" />
        </TableRow>

        <TableRow
            android:id="@+id/tr_10"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:minHeight="70dp">

            <EditText
                android:id="@+id/ed_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textEmailAddress" />
        </TableRow>

        <TableRow
            android:id="@+id/tr_117"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FFFFFF"
            android:gravity="center"
            android:minHeight="70dp"
            android:visibility="visible">

            <Button
                android:id="@+id/btn_2"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/design_default_color_secondary_variant"
                android:text="@string/btn_modificar" />

        </TableRow>

    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>